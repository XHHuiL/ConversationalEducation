.UCenter-bg {
  background-image: url(http://adweb-image.oss-cn-shanghai.aliyuncs.com/mine_bg.jpg);
  background-size: cover; 
  height: 500rpx;
  display: flex;
  justify-content: center;
  padding-top: 40rpx;
  overflow: hidden;
  position: relative;
  flex-direction: column;
  align-items: center;
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
}

.UCenter-bg text {
  opacity: 0.8;
}

.UCenter-bg .png {
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
}

.UCenter-bg .gif-wave{
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;  
  height: 100rpx;   
}

page {
  background-color: #eeeeee;
}

.cu-dialog .flex-sub {
  margin-left: 16rpx;
}